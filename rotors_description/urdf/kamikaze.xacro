<?xml version="1.0"?>
<!--
-->

<robot name="kamikaze" xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- Properties -->
  <xacro:property name="namespace" value="$(arg namespace)" />
  <xacro:property name="use_mesh_file" value="true" />
  <xacro:property name="mesh_file" value="package://rotors_description/meshes/kamikaze.dae" />
  <xacro:property name="use_body_length" value="true" />
  <xacro:property name="body_length" value="1" />
  <xacro:property name="body_width" value=".22" /> <!-- [m] -->
  <xacro:property name="body_height" value="0.45" /> <!-- [m] -->

  <!-- Properties TBC-->
  <xacro:property name="rotor_velocity_slowdown_sim" value="10" />
  <xacro:property name="mass" value="1.5" /> <!-- [kg] -->
  <xacro:property name="mass_rotor" value="0.005" /> <!-- [kg] -->
  <xacro:property name="radius_rotor" value="0.1" /> <!-- [m] -->
  <xacro:property name="motor_constant" value="8.54858e-06" /> <!-- [kg m/s^2] -->
  <xacro:property name="moment_constant" value="0.016" /> <!-- [m] -->
  <xacro:property name="time_constant_up" value="0.0125" /> <!-- [s] -->
  <xacro:property name="time_constant_down" value="0.025" /> <!-- [s] -->
  <xacro:property name="max_rot_velocity" value="1838" /> <!-- [rad/s] -->
  <xacro:property name="rotor_drag_coefficient" value="8.06428e-05" />
  <xacro:property name="rolling_moment_coefficient" value="0.000001" />

  <!-- Aerodynamics -->
  <!-- Henry's -->
  <xacro:property name="ao"           value="-0.0131" />
  <xacro:property name="cla"          value="6.4983" />
  <xacro:property name="alpha_stall"  value="0.284" />
  <xacro:property name="cla_stall"    value="-3.8268" />
  <xacro:property name="area"         value="0.040132" /> <!-- in m^2, dont know if its ok -->

  <!-- World -->
  <xacro:property name="air_density"  value="1.2041" />

  <!-- TBC - this parameters are from plane.sdf -->
  <xacro:property name="cda"          value="0.6417112299" />
  <xacro:property name="cma"          value="-1.8" />
  <xacro:property name="cda_stall"    value="-0.9233984055" />
  <xacro:property name="cma_stall"    value="0.0" />



  <!-- Property Blocks -->
  <xacro:property name="body_inertia">
    <inertia ixx="0.006" ixy="0.0" ixz="0.0" iyy="0.074" iyz="0.0" izz="0.069" /> <!-- [kg m^2] [kg m^2] [kg m^2] [kg m^2] [kg m^2] [kg m^2] -->
  </xacro:property>

  <!-- inertia of a single rotor, assuming it is a cuboid. Height=3mm, width=15mm -->
  <xacro:property name="rotor_inertia">
    <xacro:box_inertia x="${radius_rotor}" y="0.015" z="0.003" mass="${mass_rotor*rotor_velocity_slowdown_sim}" />
  </xacro:property>

  <!-- Included URDF Motors Files -->
  <xacro:include filename="$(find rotors_description)/urdf/multirotor_base.xacro" />
  
  <!-- Instantiate multirotor_base_macro once -->
  <xacro:multirotor_base_macro
    robot_namespace="${namespace}"
    mass="${mass}"
    use_body_length="${use_body_length}"
    body_length="${body_length}"
    body_width="${body_width}"
    body_height="${body_height}"
    use_mesh_file="${use_mesh_file}"
    mesh_file="${mesh_file}"
    >
    <xacro:insert_block name="body_inertia" />
  </xacro:multirotor_base_macro>

  <!-- Instantiate rotors -->
  <xacro:vertical_rotor
    robot_namespace="${namespace}"
    suffix="right"
    direction="ccw"
    motor_constant="${motor_constant}"
    moment_constant="${moment_constant}"
    parent="${namespace}/base_link"
    mass_rotor="${mass_rotor}"
    radius_rotor="${radius_rotor}"
    time_constant_up="${time_constant_up}"
    time_constant_down="${time_constant_down}"
    max_rot_velocity="${max_rot_velocity}"
    motor_number="0"
    rotor_drag_coefficient="${rotor_drag_coefficient}"
    rolling_moment_coefficient="${rolling_moment_coefficient}"
    color="Red"
    use_own_mesh="false"
    mesh="">
    <origin xyz="-0.228 0 0.059" rpy="0 0 0" />
    <xacro:insert_block name="rotor_inertia" />
  </xacro:vertical_rotor>

  <xacro:vertical_rotor
    robot_namespace="${namespace}"
    suffix="left"
    direction="cw"
    motor_constant="${motor_constant}"
    moment_constant="${moment_constant}"
    parent="${namespace}/base_link"
    mass_rotor="${mass_rotor}"
    radius_rotor="${radius_rotor}"
    time_constant_up="${time_constant_up}"
    time_constant_down="${time_constant_down}"
    max_rot_velocity="${max_rot_velocity}"
    motor_number="1"
    rotor_drag_coefficient="${rotor_drag_coefficient}"
    rolling_moment_coefficient="${rolling_moment_coefficient}"
    color="Blue"
    use_own_mesh="false"
    mesh="">
    <origin xyz="0.228 0 0.059" rpy="0 0 0" />
    <xacro:insert_block name="rotor_inertia" />
  </xacro:vertical_rotor>


  <!-- modelo servo -->

  <joint name="${namespace}/elevon_2_revolutejoint" type="continuous">
    <axis xyz="1 0 0" />
    <property name="mesh_file" value="package://rotors_description/meshes/kamikaze.dae" />
    <origin xyz="0.11 0 -0.088" rpy="0 0.17 0" />
    <parent link="${namespace}/base_link" />
    <child link="${namespace}/elevon_unit_2" />
  </joint>

  <link name="${namespace}/elevon_unit_2">
    <inertial>
      <mass value="0.1" />
      <xacro:insert_block name="body_inertia" />
    </inertial>

    <visual>
      <origin xyz="0.135 0 -0.0175" rpy="0 0 0" />
      <geometry>
        <box size="0.27 0.01 0.035"/>
        <!-- <mesh filename="package://rotors_description/meshes/elevon_left.dae"/> -->
      </geometry>
    </visual>

    <collision>
      <origin xyz="0.135 0 -0.0175" rpy="0 0 0" />
      <geometry>
        <box size="0.27 0.01 0.035"/>
      </geometry>
    </collision>
  </link>

  <gazebo>
    <plugin name="elevon_unit_2_unit_model" filename="librotors_gazebo_motor_model.so">
      <robotNamespace>${namespace}</robotNamespace>
      <jointName>${namespace}/elevon_2_revolutejoint</jointName>
      <linkName>${namespace}/elevon_unit_2</linkName>
      <turningDirection>ccw</turningDirection>
      <motorType>position</motorType>
      <commandSubTopic>command/elevon_2_angle</commandSubTopic>
      <motorNumber>0</motorNumber>
      <motorSpeedPubTopic>elevon/2/velocity</motorSpeedPubTopic>
      <motorPositionPubTopic>elevon/2/position</motorPositionPubTopic>
      <joint_control_pid>
        <p>0.5</p>
        <i>0.5</i>
        <d>0.1</d>
        <iMax>1.0</iMax>
        <iMin>-1.0</iMin>
        <cmdMax>2.0</cmdMax>
        <cmdMin>-2.0</cmdMin>
      </joint_control_pid>
    </plugin>
  </gazebo>

  <gazebo reference="${namespace}/elevon_unit_2">
    <material>Gazebo/Blue</material>
  </gazebo>

  <!-- elevon 2 -->
  <joint name="${namespace}/elevon_3_revolutejoint" type="continuous">
    <axis xyz="1 0 0" />
    <property name="mesh_file" value="package://rotors_description/meshes/kamikaze.dae" />
    <origin xyz="-0.11 0 -0.088" rpy="0 2.96 0" />
    <parent link="${namespace}/base_link" />
    <child link="${namespace}/elevon_unit_3"/>

  </joint>

  <link name="${namespace}/elevon_unit_3">
    <inertial>
      <mass value="0.1" />
      <xacro:insert_block name="body_inertia" />
    </inertial>
    <visual>
        <origin xyz="0.135 0 0.0175" rpy="0 0 0" />
        <geometry>
          <box size="0.27 0.01 0.035"/>
          <!-- <mesh filename="package://rotors_description/meshes/elevon_left.dae"/> -->
        </geometry>
      </visual>

      <collision>
        <origin xyz="-0.135 0 -0.0175" rpy="0 0 0" />
        <geometry>
          <box size="0.27 0.01 0.035"/>
        </geometry>
      </collision>
  </link>

  <gazebo reference="${namespace}/elevon_unit_3">
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo>
    <plugin name="elevon_unit_3_unit_model" filename="librotors_gazebo_motor_model.so">
      <robotNamespace>${namespace}</robotNamespace>
      <jointName>${namespace}/elevon_3_revolutejoint</jointName>
      <linkName>${namespace}/elevon_unit_3</linkName>
      <turningDirection>ccw</turningDirection>
      <motorType>position</motorType>
      <commandSubTopic>command/elevon_3_angle</commandSubTopic>
      <motorNumber>1</motorNumber>
      <motorSpeedPubTopic>elevon/3/velocity</motorSpeedPubTopic>
      <motorPositionPubTopic>elevon/3/position</motorPositionPubTopic>
      <joint_control_pid>
        <p>0.5</p>
        <i>0.5</i>
        <d>0.1</d>
        <iMax>1.0</iMax>
        <iMin>-1.0</iMin>
        <cmdMax>2.0</cmdMax>
        <cmdMin>-2.0</cmdMin>
      </joint_control_pid>
    </plugin>
  </gazebo>


  <!-- liftDrag 1 -->
  <gazebo>
    <plugin name="wing_0" filename="libLiftDragPlugin.so">
      <a0>${a0}</a0>
      <cla>${cla}</cla>
      <cda>${cda}</cda>
      <cma>${cma}</cma>
      <alpha_stall>${alpha_stall}</alpha_stall>
      <cla_stall>${cla_stall}</cla_stall>
      <cda_stall>${cda_stall}</cda_stall>
      <cma_stall>${cma_stall}</cma_stall>
      <cp>-0.228 0 0.0318</cp><!-- FIXME!!!! -->
      <area>${area}</area>
      <air_density>${air_density}</air_density>
      <forward>0 0 1</forward>
      <upward>0 1 0</upward>
      <link_name>${namespace}/base_link</link_name>
      <control_joint_name>${namespace}/elevon_3_revolutejoint</control_joint_name>
      <control_joint_rad_to_cl>${control_joint_rad_to_cl}</control_joint_rad_to_cl>
      <velocity_joint_name>${namespace}/rotor_0_joint</velocity_joint_name>
      <rotorVelocitySlowdownSim>${rotor_velocity_slowdown_sim}</rotorVelocitySlowdownSim>
      <motorConstant>${motor_constant}</motorConstant>
    </plugin>
  </gazebo>

  <!-- liftDrag 2 -->
  <gazebo>
    <plugin name="wing_1" filename="libLiftDragPlugin.so">
      <a0>${a0}</a0>
      <cla>${cla}</cla>
      <cda>${cda}</cda>
      <cma>${cma}</cma>
      <alpha_stall>${alpha_stall}</alpha_stall>
      <cla_stall>${cla_stall}</cla_stall>
      <cda_stall>${cda_stall}</cda_stall>
      <cma_stall>${cma_stall}</cma_stall>
      <cp>0.228 0 0.0318</cp><!-- FIXME!!!! -->
      <area>${area}</area>
      <air_density>${air_density}</air_density>
      <forward>0 0 1</forward>
      <upward>0 1 0</upward>
      <link_name>${namespace}/base_link</link_name>
      <control_joint_name>${namespace}/elevon_2_revolutejoint</control_joint_name>
      <control_joint_rad_to_cl>${control_joint_rad_to_cl}</control_joint_rad_to_cl>
      <velocity_joint_name>${namespace}/rotor_1_joint</velocity_joint_name>
      <rotorVelocitySlowdownSim>${rotor_velocity_slowdown_sim}</rotorVelocitySlowdownSim>
      <motorConstant>${motor_constant}</motorConstant>
    </plugin>
  </gazebo>


</robot>
